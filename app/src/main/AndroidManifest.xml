<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.charity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".physical"></activity>
        <activity android:name=".sworker" />
        <activity android:name=".farmer" />
        <activity android:name=".poor" />
        <activity android:name=".covid" />
        <activity android:name=".disaster" />
        <activity android:name=".women" />
        <activity android:name=".medical" />
        <activity android:name=".oldage" />
        <activity android:name=".children" />
        <activity android:name=".animal" />
        <activity android:name=".category" />
        <activity android:name=".browser" />
        <activity android:name=".Charity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>